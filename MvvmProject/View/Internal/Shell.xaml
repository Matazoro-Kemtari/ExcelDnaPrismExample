<UserControl x:Class="MvvmProject.View.Internal.Shell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MvvmProject"
             xmlns:iview="clr-namespace:MvvmProject.View.Internal"
             xmlns:prism="http://www.codeplex.com/prism"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ItemsControl prism:RegionManager.RegionName="{x:Static local:RegionNames.ShellContentRegion}"></ItemsControl>
    </Grid>
</UserControl>
